<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- K.Aoki 2013 -->
<PropertyList>

  <path>fuel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>GalsL10</object-name>
    <object-name>GalsL1</object-name>
    <object-name>GalsF10</object-name>
    <object-name>GalsF1</object-name>
    <object-name>GalsR10</object-name>
    <object-name>GalsR1</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>GalsL10</object-name>
    <object-name>GalsL1</object-name>
    <object-name>GalsF10</object-name>
    <object-name>GalsF1</object-name>
    <object-name>GalsR10</object-name>
    <object-name>GalsR1</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0 </red>
      <green> 1 </green>
      <blue>  2 </blue>
    </emission>
  </animation><step>1</step>

  <animation>
    <type>textranslate</type>
    <object-name>GalsL10</object-name>
    <property>/consumables/fuel/tank[1]/level-m3</property>
    <factor> 10 </factor>
    <step> 0.01 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>GalsL1</object-name>
    <property>/consumables/fuel/tank[1]/level-m3</property>
    <factor> 100 </factor>
    <step> 0.001 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>GalsF10</object-name>
    <property>/consumables/fuel/tank[0]/level-m3</property>
    <factor> 10 </factor>
    <step> 0.01 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>GalsF1</object-name>
    <property>/consumables/fuel/tank[0]/level-m3</property>
    <factor> 100 </factor>
    <step> 0.001 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>GalsR10</object-name>
    <property>/consumables/fuel/tank[2]/level-m3</property>
    <factor> 10 </factor>
    <step> 0.01 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>GalsR1</object-name>
    <property>/consumables/fuel/tank[2]/level-m3</property>
    <factor> 100 </factor>
    <step> 0.001 </step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
