<?xml version="1.0"?>

<PropertyList>

 <path>CHT.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Ptr</object-name>
  <object-name>PtrTip</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Ptr</object-name>
  <object-name>PtrTip</object-name>
  <property>/instrumentation/engine/cht-degf</property>
  <factor>0.1</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>-0.022</z-m>
  </center>
 </animation>

</PropertyList>
