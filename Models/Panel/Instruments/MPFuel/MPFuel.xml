<?xml version="1.0"?>

<PropertyList>

 <path>MPFuel.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>ptrTip</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ptrTip</object-name>
  <object-name>ptrCtr</object-name>
  <property>/engines/engine/mp-inhg</property>
  <interpolation>
   <entry><ind>8.86</ind><dep>-175</dep></entry>
   <entry><ind>44.3</ind><dep>150</dep></entry>
  </interpolation>
  <min>-175</min>
  <max>150</max>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

</PropertyList>
