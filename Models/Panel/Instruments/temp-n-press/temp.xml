<?xml version="1.0"?>

<PropertyList>

 <path>temp.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>temp.PtrT</object-name>
<!--
  <object-name>press1.PtrT</object-name>
  <object-name>press2.PtrT</object-name>
-->
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>temp.PtrT</object-name>
  <object-name>temp.PtrC</object-name>
  <property>/engines/engine/oil-temperature-degf</property>
  <interpolation>
   <entry><ind>32</ind><dep>0</dep></entry>
   <entry><ind>248</ind><dep>120</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0.009</z-m>
  </center>
 </animation>

