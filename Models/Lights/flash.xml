<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>flash.ac</path>

  <animation>
    <object-name>flash</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>flash</object-name>
  </animation>

  <animation>
    <type>flash</type>
    <object-name>flash</object-name>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <power>5</power>
    <factor>1</factor>
    <offset>0</offset>
    <two-sides type="bool">false</two-sides>
    <min>0.05</min>
    <max>1</max>
  </animation>

</PropertyList>
